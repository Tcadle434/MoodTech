{
	"expo": {
		"name": "MoodTech",
		"slug": "moodtech",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "moodtech",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.moodtech.app",
			"infoPlist": {
				"NSHealthShareUsageDescription": "This app requires access to your health data in order to correlate your physical activity with your mood.",
				"NSHealthUpdateUsageDescription": "This app requires access to your health data in order to correlate your physical activity with your mood."
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.moodtech.app"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			[
				"expo-dev-client",
				{
					"ios": {
						"entitlements": {
							"com.apple.developer.healthkit": true,
							"com.apple.developer.healthkit.access": [
								"health-records",
								"clinical-health-records"
							]
						}
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
