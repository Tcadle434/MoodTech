{
	"expo": {
		"name": "MoodTech",
		"slug": "moodtech",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "moodtech",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.moodtech.app.dev",
			"infoPlist": {
				"NSHealthShareUsageDescription": "Allow MoodTech to access your health data.",
				"NSHealthUpdateUsageDescription": "Allow MoodTech to update your health data."
			},
			"entitlements": {
				"com.apple.developer.healthkit": true,
				"com.apple.developer.healthkit.access": []
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.moodtech.app.dev"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"react-native-health",
				{
					"healthSharePermission": "Allow MoodTech to access your health data.",
					"healthUpdatePermission": "Allow MoodTech to update your health data.",
					"isClinicalDataEnabled": true,
					"healthClinicalDescription": "Allow MoodTech to access your clinical health records."
				}
			],
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			[
				"expo-dev-client",
				{
					"ios": {
						"entitlements": {
							"com.apple.developer.healthkit": true,
							"com.apple.developer.healthkit.access": [
								"health-records",
								"clinical-health-records"
							]
						}
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
